{
    "builders": {
        "react": "3.x"
    },

    "dependencies": {
        "vtex.store-components": "3.x"
    },

    "store.product": {
        "children": [
            ".productContent"
        ]
    },

    ".productContent": {
        "children": [
            + "product-images"
        ]
    },

    "product-images": {
        "props": {
            "displayThumbnailsArrows": true
        }
    }
}